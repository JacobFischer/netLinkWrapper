{
  "name": "netlinkwrapper",
  "version": "2.0.0",
  "description": "This is a simple node-gyp module wrapper for the C++ library netLink v1.0.0_pre6",
  "author": "JacobFischer <jacob.t.fischer@gmail.com>",
  "license": "MIT",
  "main": "index.js",
  "types": "index.d.ts",
  "gypfile": true,
  "bugs": {
    "url": "https://github.com/JacobFischer/netlinkwrapper/issues"
  },
  "homepage": "https://jacobfischer.github.io/netlinkwrapper/",
  "keywords": [
    "net",
    "sync",
    "synchronous",
    "tcp",
    "socket",
    "network",
    "addon"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/JacobFischer/netlinkwrapper.git"
  },
  "dependencies": {
    "bindings": "1.5.0",
    "nan": "2.14.1"
  },
  "devDependencies": {
    "@types/bindings": "1.3.0",
    "@types/eslint": "7.2.0",
    "@types/jest": "26.0.4",
    "@types/node": "14.0.23",
    "@typescript-eslint/eslint-plugin": "3.6.1",
    "eslint": "7.4.0",
    "eslint-config-jacobfischer": "git://github.com/JacobFischer/eslint-config.git#94bffc94da2336b08dd5159a1ab4c93525c46d4b",
    "eslint-config-jacobfischer-jest": "git://github.com/JacobFischer/eslint-config-jest.git#a00ba9efe989f30e990c74f115ff2762f66a4bff",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jest": "23.18.0",
    "eslint-plugin-jsdoc": "29.2.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-prettier": "3.1.4",
    "jaguarjs-jsdoc": "github:JacobFischer/jaguarjs-jsdoc",
    "jest": "26.1.0",
    "jest-cli": "26.1.0",
    "jsdoc": "3.6.4",
    "npm-check-updates": "7.0.1",
    "prettier": "2.0.5",
    "shx": "0.3.2",
    "ts-jest": "26.1.2",
    "ts-node": "8.10.2",
    "ts-typed-events": "2.0.0",
    "typescript": "3.9.7"
  },
  "scripts": {
    "clean": "npm run clean:build && npm run clean:docs",
    "clean:build": "shx rm -rf build/ dist/",
    "clean:docs": "shx rm -rf docs/",
    "purge": "npm run clean && shx rm -rf node_modules/ && rm -rf package-lock.json",
    "docs": "jsdoc main.js -r ./README.md -t node_modules/jaguarjs-jsdoc -c conf.json -d docs",
    "docs:predeploy": "shx touch docs/.nojekyll",
    "build": "npm run build:ts && npm run build:cpp",
    "build:ts": "tsc --project ./tsconfig.build.json",
    "build:cpp": "node-gyp rebuild",
    "lint": "eslint ./",
    "prettier:base": "prettier **/*.{js,ts}",
    "prettier": "npm run prettier:base -- --write",
    "prettier:check": "npm run prettier:base -- --check",
    "test": "jest --env=node --config ./test/jest.config.js --colors --coverage test",
    "ncu": "ncu -u"
  },
  "files": [
    "src/cpp/",
    "dist/"
  ]
}